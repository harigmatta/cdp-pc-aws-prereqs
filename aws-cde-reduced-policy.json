{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ElasticFileSystem",
            "Action": [
                "elasticfilesystem:*"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        },
        {
            "Sid": "CloudWatch",
            "Action": [
                "cloudwatch:GetMetricData"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        },
        {
            "Sid": "ElasticLoadBalancing",
            "Action": [
                "elasticloadbalancing:*"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        },
        {
            "Sid": "RelationalDatabaseService",
            "Action": [
                "rds:*"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        }
    ]
}
